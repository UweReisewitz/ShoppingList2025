<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:ShoppingList2025"
             xmlns:shared="clr-namespace:ShoppingList2025.Shared;assembly=ShoppingList2025.Shared"
             x:Class="ShoppingList2025.MainPage"
             BackgroundColor="{DynamicResource PageBackgroundColor}">

    <local:ApplicationBlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <local:ApplicationBlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type shared:Routes}" />
        </local:ApplicationBlazorWebView.RootComponents>
    </local:ApplicationBlazorWebView>

</ContentPage>
